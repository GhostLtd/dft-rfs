{% extends 'admin/base-with-nav.html.twig' %}

{% set translation_prefix = "admin.domestic.export-success" %}
{% set headingParams = {
    year: year,
    quarter: quarter
} %}

{%- block adminContent -%}
    <p class="govuk-body">Export successfully generated for {{ year }} Q{{ quarter }}</p>

    <a href="{{ url('admin_domestic_export_download', {year: year, quarter: quarter}) }}" class="govuk-button govuk-!-margin-bottom-0">Download</a>
    <a href="{{ url('admin_domestic_export_list') }}" class="govuk-button govuk-button--secondary govuk-!-margin-bottom-0">Back to exports list</a>
{%- endblock -%}